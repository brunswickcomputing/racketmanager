{
  "name": "racketmanager/racketmanager",
  "description": "RacketManager WordPress plugin",
  "type": "wordpress-plugin",
  "license": "GPL-2.0-or-later",
  "require": {
    "php": ">=8.3"
  },
  "autoload": {
    "psr-4": {
      "Racketmanager\\": "src/php/"
    },
    "classmap": [
      "include/"
    ]
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true
  }
}